<h1>Listing map_tasks</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Order</th>
    <th>Status</th>
    <th>Task</th>
    <th>Match</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @map_tasks.each do |map_task| %>
  <tr>
    <td><%= map_task.title %></td>
    <td><%= map_task.order %></td>
    <td><%= map_task.status %></td>
    <td><%= map_task.task_id %></td>
    <td><%= map_task.match_id %></td>
    <td><%= link_to 'Show', map_task %></td>
    <td><%= link_to 'Edit', edit_map_task_path(map_task) %></td>
    <td><%= link_to 'Destroy', map_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Map task', new_map_task_path %>
